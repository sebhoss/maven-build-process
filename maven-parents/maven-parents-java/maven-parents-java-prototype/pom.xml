  <artifactId>maven-parents-java-prototype</artifactId>
  <packaging>pom</packaging>

  <!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
  <!--                                PROPERTIES                               -->
  <!-- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
  <properties>
    <jacoco.lineCoverage>0.00000</jacoco.lineCoverage>
    <pit.mutationCoverage>0</pit.mutationCoverage>
    <pmd.skip>true</pmd.skip>
    <findbugs.skip>true</findbugs.skip>
    <failBuildOnDuplicates>false</failBuildOnDuplicates>
    <failBuildOnDependencyVersionProblem>false</failBuildOnDependencyVersionProblem>
    <failBuildOnDependencyProblem>false</failBuildOnDependencyProblem>
  </properties>

</project>
